         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="phoneNumber" layoutX="339.0" layoutY="238.0" prefHeight="27.0" prefWidth="299.0" text="Номер телефона">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="editButton" layoutX="339.0" layoutY="344.0" mnemonicParsing="false" onAction="#onClickEdit" text="Редактировать">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="587.0" layoutY="344.0" mnemonicParsing="false" onAction="#exit" prefHeight="39.0" prefWidth="60.0" text="Выход" />
   </children>
</AnchorPane>
