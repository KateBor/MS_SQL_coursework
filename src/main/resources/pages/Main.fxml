<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="423.0" prefWidth="677.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="32.0" layoutY="39.0" text="Выберите команду:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="339.0" layoutY="39.0" text="Пользователь:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="32.0" layoutY="77.0" onAction="#onClickFunc" prefHeight="25.0" prefWidth="243.0" promptText="Команды" />
      <Label fx:id="role" layoutX="467.0" layoutY="39.0" prefHeight="27.0" prefWidth="199.0" text="role">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="name" layoutX="339.0" layoutY="76.0" prefHeight="27.0" prefWidth="299.0" text="имя">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="surname" layoutX="339.0" layoutY="115.0" prefHeight="27.0" prefWidth="299.0" text="фамилия">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="email" layoutX="339.0" layoutY="155.0" prefHeight="27.0" prefWidth="299.0" text="почта">
         <font>
            <Font size="18.0" />
